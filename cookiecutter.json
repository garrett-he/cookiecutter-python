{
    "project_name": "Project Name",
    "project_slug": "{{ cookiecutter.project_name | slugify }}",
    "project_package": "{{ cookiecutter.project_slug.replace('-', '_') }}",
    "project_description": "New project generated by cookiecutter-python",
    "project_version": "0.1.0",
    "project_keywords": "",
    "author_name": "Your Name",
    "author_email": "your.name@email.com",
    "license_id": [
        "Apache-2.0",
        "BSD-3-Clause",
        "GPL-3.0",
        "LGPL-3.0",
        "MIT",
        "MPL-2.0",
        "Unlicense"
    ],
    "license_fullname": "{{ cookiecutter.author_name }} <{{ cookiecutter.author_email }}>",
    "license_year": "{% now 'utc', '%Y' %}",
    "github_path": "{{ cookiecutter.author_name.lower() | slugify }}/{{ cookiecutter.project_slug }}",
    "python_version": "^3.10,<3.13",
    "_copy_without_render": [
        ".github/*"
    ]
}
